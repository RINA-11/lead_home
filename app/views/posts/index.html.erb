<div class="posts-index-wrapper">
  <div class="container">
    <% @posts.each do |post| %>
      <div>
        <%= link_to post_path(id: post.id) do %>
          <%= post.content %>
          <%= image_tag post.image.post_image1.url(:thumb150) %>
        <% end %>
      </div>
    <% end %>
    <%= paginate @posts %>
  </div>
</div>